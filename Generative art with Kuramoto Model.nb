  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"phiInit", ",", "omega", ",", "trajs"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Clear", "[", "phi", "]"}], ";", " ", 
     RowBox[{"Clear", "[", "t", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"SeedRandom", "[", "0", "]"}], ";", "\n", 
     RowBox[{"phiInit", "=", " ", 
      RowBox[{"RandomReal", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "Pi"}], ",", "Pi"}], "}"}], ",", "n"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"trajs", "=", 
      RowBox[{"NDSolve", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"phi", "[", "i", "]"}], "'"}], "[", "t", "]"}], 
              "\[Equal]", " ", 
              RowBox[{
               RowBox[{"ep", "/", "n"}], " ", 
               RowBox[{"Sum", "[", 
                RowBox[{
                 RowBox[{"Sin", "[", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"phi", "[", "j", "]"}], "[", "t", "]"}], "-", 
                   RowBox[{
                    RowBox[{"phi", "[", "i", "]"}], "[", "t", "]"}]}], "]"}], 
                 ",", 
                 RowBox[{"{", 
                  RowBox[{"j", ",", "n"}], "}"}]}], "]"}]}]}], ",", 
             RowBox[{
              RowBox[{
               RowBox[{"phi", "[", "i", "]"}], "[", "0", "]"}], "\[Equal]", 
              RowBox[{"phiInit", "[", 
               RowBox[{"[", "i", "]"}], "]"}]}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "n"}], "}"}]}], "]"}], "}"}], ",", 
        RowBox[{"Table", "[", 
